<div class="jumbotron">
  <div id="stripe">
    <h1 class="col-md-offset-1" id='main-header'>Find a vendor</h1>
  </div>
</div>
  <div id="category-buttons" class="col-md-offset-2 col-md-8">
    <p>
      <button type="button" id="plastic-injection" class="btn btn-lg">Plastic Injection Molding</button>
      <button type="button" id="laser-cutting" class="btn btn-lg" >Laser/Water-jet Cutting</button>
      <button type="button" id="prototype" class="btn  btn-lg">Prototype Building</button>
      <button type="button" id= "3d-printing" class="btn btn-lg">3d Printing</button>
      <button type="button" id= "packaging" class="btn btn-lg">Packaging</button>
      <button type="button" id= "design-services" class="btn btn-lg">Design services</button>
      <button type="button" id= "electronics" class="btn btn-lg">Electronics Design</button>
      <button type="button" id="clear-filters" class="btn btn-lg clear-filter"><span class="glyphicon glyphicon-remove"></span>Clear Filters</button>
    </p>
  </div>

  <div id="add-new" class="col-md-1">
    <h3>Add a new contact</h3>
    <p><a href="/sign-in">Sign-in</a> or <a href="/sign-up">Sign-up</a></p>
  </div>

    <div class="contacts container-fluid">
    {{#each EngineeringContacts}}
      <div class="each-contact panel panel-default col-md-8 col-md-offset-2">
       <div class="panel-heading">
          <h2 class="panel-title">{{this.Name}}</h2>
        </div>
        <div class="panel-body">
          <a href={{this.Website}}>{{this.Website}}</a><br>
          <a href="mailto:{{this.Email}}">{{this.Email}}</a>
          <p>{{this.Phone}}</p>
          <p>{{this.Address}}</p>
          {{#each this.services}}
            <div class="service-tags">
              <h3>
                <span class="label label-success">{{this}}</span>
              </h3>
            </div>
          {{/each}}
        </div>
      </div>
    {{/each}}
  </div>
